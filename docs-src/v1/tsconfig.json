{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": "./",
    "strictNullChecks": true,
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@data/*": [
        "src/data/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
    }
  }
}